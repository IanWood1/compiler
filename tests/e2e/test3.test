// E2E test - nested loops with function calls
// Converted from legacy test3.cpp/test3.program
// Expected return value: 150

int64 test3add(int64 a, int64 b){
  return a + b
}

int64 test3(int64[5] vec, int64 len){
  int64 sum, i, j
  sum = 0
  i = 0
  while(i < len){
    j = 0
    while(j < len){
      sum = test3add(vec[i], test3add(vec[j], sum))
      j = j + 1
    }
    i = i + 1
  }
  return sum
}

